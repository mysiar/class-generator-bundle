{
    "name" : "slawomirkania/class-generator-bundle",
    "description" : "Class Generator",
    "type" : "symfony-bundle",
    "authors" : [
        {
            "name" : "Slawomir Kania",
            "email" : "slawomir.kania1@gmail.com"
        }
    ],
    "keywords" : [
        "class", "entity", "generator", "test", "interface"
    ],
    "license" : [
        "MIT"
    ],
    "require" : {
        "php": ">=5.3.9",
        "symfony/framework-bundle": "~2.3|~3.0",
        
        "doctrine/common": "~2.2",
        "jms/serializer": "~1.0",
        "symfony/console": "~2.3|~3.0",
        "symfony/filesystem": "~2.3|~3.0",
        "symfony/yaml": "~2.2|~3.0",
        "symfony/validator": "~3.1"
    },
    "require-dev" : {
        "phpunit/phpunit": "~4.0"
    },
    "autoload" : {
        "psr-0" : {
            "HelloWordPl\\SimpleEntityGeneratorBundle\\" : "",
            "Tests\\HelloWordPl\\SimpleEntityGeneratorBundle\\" : "Tests/src"
        }
    },
    "target-dir" : "HelloWordPl/SimpleEntityGeneratorBundle",
    "extra" : {
        "branch-alias" : {
            "dev-master" : "1.x.x-dev"
        }
    }
}
